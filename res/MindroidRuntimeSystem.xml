<?xml version='1.0' encoding='UTF-8' ?>
<runtime>
    <nodes>
        <node id="1">
            <server scheme="mindroid" uri="tcp://localhost:12345" class="mindroid.runtime.system.Mindroid" />
            <server scheme="xmlrpc" uri="tcp://localhost:1234" class="mindroid.runtime.system.plugins.xmlrpc.XmlRpc" />
        </node>
        <node id="42">
            <server scheme="mindroid" uri="tcp://localhost:54321" class="mindroid.runtime.system.Mindroid" />
            <server scheme="xmlrpc" uri="tcp://localhost:4321" class="mindroid.runtime.system.plugins.xmlrpc.XmlRpc" />
        </node>
    </nodes>

    <serviceDiscovery>
        <node id="1">
            <service id="1" name="eliza">
                <announcement descriptor="mindroid://interfaces/examples/eliza/IEliza" />
                <announcement descriptor="xmlrpc://interfaces/examples/eliza/IEliza" />
            </service>
            <service id="2" name="locationService">
                <announcement descriptor="mindroid://interfaces/mindroid/location/ILocationManager" />
            </service>
        </node>
    </serviceDiscovery>
</runtime>
